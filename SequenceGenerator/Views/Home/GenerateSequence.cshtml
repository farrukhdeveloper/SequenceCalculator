@model SequenceGenerator.Models.sequenceModel
@{
    ViewBag.Title = "Generate Sequence";
}

<h2>Generate Sequence</h2>


@using (Html.BeginForm("processSequence", "Home", FormMethod.Post))
{

    @Html.ValidationSummary(true)
    <div class="row">
        <div class="col-md-12">
            <section>
                <div class="form-group">
                    <div class="col-md-2 ">Please enter some limit value: </div>
                    <div class="col-md-3">

                        @Html.EditorFor(m => m.limitValue, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(m => m.limitValue)
                    </div>
                    <div class="col-md-2"><input type="submit" name="Submit" value="Submit" class="btn btn-primary" /></div>
                </div>

                <div class="form-group">
                    <div class="col-md-2"> </div>
                    <div class="col-md-3">
                        
                    </div>
                    <div class="col-md-2">
                    </div>
                </div>

            </section>
        </div>
    </div>

}
